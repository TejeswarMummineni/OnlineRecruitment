@model IPagedList<BussinessEntites.JobApplicantEntites>
@{
    ViewBag.Title = "PLacedApplicants";
}
@using PagedList.Mvc;
@using PagedList;
<link href="~/Content/Site.css" rel="stylesheet" />
<h2>PLacedApplicants</h2>
<table>
    @foreach (var item in Model)
    {
        <tr>
            <td>
                <h1> @Html.DisplayFor(model => item.CandidateId)</h1>
            </td>
            <td>
                @Html.DisplayFor(model => item.Name)
            </td>
            <td>
                @Html.DisplayFor(model => item.Dob)
            </td>
            <td>
                @Html.DisplayFor(model => item.Phone)
            </td>
            <td>
                @Html.DisplayFor(model => item.Email)
            </td>
            <td>
                @Html.DisplayFor(model => item.Gender)
            </td>
            <td>
                @Html.DisplayFor(model => item.UniversityName)
            </td>
            <td>
                @Html.DisplayFor(model => item.Location)
            </td>
        </tr>

    }
</table>
<div>
    @Html.PagedListPager(Model, page => Url.Action("PLacedApplicants", new { page }))

</div>
